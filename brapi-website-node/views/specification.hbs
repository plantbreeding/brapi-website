<section class="module-small">
    <div class="container">

        <div class="row" style="margin-bottom: 10px;">
            <h2>BrAPI V2</h2>
        </div>
        {{#each v2}}
        <div
            class="row brapi-section {{#if isCurrent}} current-brapi-section {{/if}} {{#if isPreview}} preview-brapi-section {{/if}}" style="padding: 0 10px;">
            <h3 class="brapi-section-title col-sm-3 col-md-2">{{version}}<br />{{status}}</h3>
            <div class="col-sm-9 col-md-10">
                <div class="row brapi-section">
                    <div class="brapi-section-description">
                        <p style="margin: 0 10px 0 10px;">{{{description}}}</p>
                    </div>
                    {{#each links}}
                    <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                        <a target="_blank" href="{{URL}}">
                            <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                type="button">{{label}}</button>
                        </a>
                    </div>
                    {{/each}}
                </div>
            </div>

            <div class="brapp-details-btn" style="margin: 0 0 5px;">
                <button id="toggle-{{ version }}" class="btn btn btn-round" type="button"
                    onclick="toggleVisible('id-{{ version }}', 'toggle-{{ version }}', 'Details', 'Details')">
                    {{#if isCurrent}}
                    <span class="mdi mdi-chevron-double-up"></span> Details <span
                        class="mdi mdi-chevron-double-up"></span>
                    {{else}}
                    <span class="mdi mdi-chevron-double-down"></span> Details <span
                        class="mdi mdi-chevron-double-down"></span>
                    {{/if}}
                </button>
            </div>

            <div id="id-{{ version }}" class="row brapi-section" style="background-color: white; {{#unless isCurrent}}display: none;{{/unless}}">
                <div class="col-sm-12">
                    {{#each modules}}
                    <div class="row brapi-section">
                        <h4 class="brapi-section-subtitle col-sm-3 col-lg-2">{{name}}</h4>
                        <div class="col-sm-9 col-lg-10">
                            <div class="brapi-section-description">{{description}}</div>
                            <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                                <a target="_blank" href="{{githubURL}}">
                                    <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                        type="button">GitHub</button>
                                </a>
                            </div>
                            <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                                <a target="_blank" href="{{apiaryURL}}">
                                    <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                        type="button">Apiary</button>
                                </a>
                            </div>
                            <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                                <a target="_blank" href="{{swaggerhubURL}}">
                                    <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                        type="button">SwaggerHub</button>
                                </a>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>

        </div>
        {{/each}}

        <hr class="divider-w">

        <div class="row">
            <h2>BrAPI V1</h2>
        </div>
        <div class="row">
            <div class="col-sm-12">
                {{#each v1}}
                <div class="row brapi-section">
                    <h3 class="brapi-section-title col-sm-3 col-md-2">{{version}}<br />{{status}}</h3>
                    <div class="col-sm-9 col-md-10">
                        <div class="brapi-section-description">{{description}}</div>
                        <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                            <a target="_blank" href="{{githubURL}}">
                                <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                    type="button">GitHub</button>
                            </a>
                        </div>
                        <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                            <a target="_blank" href="{{apiaryURL}}">
                                <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                    type="button">Apiary</button>
                            </a>
                        </div>
                        <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                            {{#if swaggerhubURL}}
                            <a target="_blank" href="{{swaggerhubURL}}">
                                <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                    type="button">SwaggerHub</button>
                            </a>
                            {{else}}
                            <div class="btn-sm btn-round col-sm-12" style="background-color: lightgray; height: 30px;">
                            </div>
                            {{/if}}
                        </div>
                        <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                            <a target="_blank" href="{{releaseNotesURL}}">
                                <button class="btn-sm btn-success btn-border-d btn-round col-sm-12"
                                    type="button">Release Notes</button>
                            </a>
                        </div>
                        <div class="link-btn col-sm-6 col-sm-5 col-md-3 col-lg-2">
                            {{#if changeLogURL}}
                            <a target="_blank" href="{{changeLogURL}}">
                                <button class="btn-sm btn-success btn-border-d btn-round col-sm-12" type="button">Change
                                    Log</button>
                            </a>
                            {{else}}
                            <div class="btn-sm btn-round col-sm-12" style="background-color: lightgray; height: 30px;">
                            </div>
                            {{/if}}
                        </div>
                    </div>
                </div>
                {{/each}}


            </div>
        </div>

        <hr class="divider-w">

        <div class="row">
            <h2>General Reference Documentation</h2>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="row link-btn">
                    <a target="_blank"
                        href="https://github.com/plantbreeding/API/blob/master/Specification/GeneralInfo/URL_Structure.md">
                        <button class="btn-sm btn-success btn-border-d btn-round col-sm-3" type="button">URL
                            Structure</button>
                    </a>
                </div>
                <div class="row link-btn">
                    <a target="_blank"
                        href="https://github.com/plantbreeding/API/blob/master/Specification/GeneralInfo/Response_Structure.md">
                        <button class="btn-sm btn-success btn-border-d btn-round col-sm-3" type="button">Response
                            Structure</button>
                    </a>
                </div>
                <div class="row link-btn">
                    <a target="_blank"
                        href="https://github.com/plantbreeding/API/blob/master/Specification/GeneralInfo/Date_Time_Encoding.md">
                        <button class="btn-sm btn-success btn-border-d btn-round col-sm-3" type="button">Date/Time
                            Encoding</button>
                    </a>
                </div>
                <div class="row link-btn">
                    <a target="_blank"
                        href="https://github.com/plantbreeding/API/blob/master/Specification/GeneralInfo/Location_Encoding.md">
                        <button class="btn-sm btn-success btn-border-d btn-round col-sm-3" type="button">Location
                            Encoding</button>
                    </a>
                </div>
                <div class="row link-btn">
                    <a target="_blank"
                        href="https://github.com/plantbreeding/API/blob/master/Specification/GeneralInfo/Error_Handling.md">
                        <button class="btn-sm btn-success btn-border-d btn-round col-sm-3" type="button">Error
                            Handling</button>
                    </a>
                </div>
                <div class="row link-btn">
                    <a target="_blank"
                        href="https://github.com/plantbreeding/API/blob/master/Specification/GeneralInfo/Search_Services.md">
                        <button class="btn-sm btn-success btn-border-d btn-round col-sm-3" type="button">Search
                            Services</button>
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>